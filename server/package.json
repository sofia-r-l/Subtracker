{
  "name": "subtracker-server",
  "version": "0.1.0",
  "private": true,
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:studio": "dotenv -e .env -- prisma studio",
    "db:seed": "ts-node-dev --transpile-only prisma/seed.ts",
    "test:api": "ts-node-dev --transpile-only src/test.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "subtracker-frontend": "file:..",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.2",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.0"
  }
}
